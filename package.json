{
  "name": "medusa-plugin-ultimate",
  "version": "0.0.6",
  "description": "Medusa Plugin Ultimate is a plugin made for medusa that give super powers to your entities.",
  "author": "Raiden",
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/raideno/medusa-plugin-ultimate"
  },
  "license": "MIT",
  "keywords": [
    "medusa-plugin-admin",
    "medusa-plugin-other",
    "medusa-plugin",
    "dashboard",
    "user-interface",
    "entity-management",
    "CRUD",
    "configuration",
    "customization",
    "routing",
    "data-management",
    "ecommerce",
    "typescript",
    "store",
    "headless-commerce",
    "database"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env npm run build && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "cross-env npm run build:server && medusa develop",
    "build": "cross-env npm run clean && npm run build:server && npm run build:admin",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.json",
    "build:admin": "cross-env medusa-admin build",
    "prepare": "cross-env NODE_ENV=production npm run build:server && medusa-admin bundle"
  },
  "peerDependencies": {
    "@medusajs/icons": "^1.0.0",
    "@medusajs/medusa": "^1.16.0",
    "@medusajs/ui": "^2.2.0",
    "@medusajs/admin": "^7.1.1",
    "medusa-react": "9.0.4",
    "swr": "^2.2.4",
    "@uiw/react-md-editor": "^3.24.1"
  },
  "dependencies": {
    "@medusajs/admin": "^7.1.1",
    "@medusajs/icons": "^1.0.0",
    "@medusajs/medusa": "^1.16.0",
    "@tanstack/react-query": "4.22.0",
    
    "babel-preset-medusa-package": "^1.1.13",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "express": "^4.17.2",
    "install": "^0.13.0",

    "medusa-interfaces": "^1.3.7",
    
    "prism-react-renderer": "^2.0.4",
    "react-router-dom": "^6.16.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
